{
  "name": "graph-gallery",
  "version": "2.0.1",
  "description": "GraphGists Portal",
  "neo4jDesktop": {
    "apiVersion": "^1.4.0"
  },
  "icons": [
    {
      "src": "./icon.png",
      "type": "png"
    }
  ],
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@auth0/auth0-react": "^1.0.0",
    "@clampy-js/react-clampy": "^1.3.0",
    "@craco/craco": "^5.8.0",
    "@hookstate/core": "^3.0.1",
    "@sentry/react": "^6.2.1",
    "@sentry/tracing": "^6.2.1",
    "apollo-upload-client": "^14.1.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.16.0",
    "d3": "^3.5.12",
    "formik": "^2.1.5",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "http-proxy-middleware": "^0.20.0",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "node-sass": "^4.14.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.1.0",
    "react-jss": "^10.3.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.1",
    "react-semantic-ui-datepickers": "^2.12.0",
    "react-share": "^4.3.1",
    "sanitize-html": "^2.3.2",
    "semantic-ui-calendar-react": "^0.15.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "underscore": "^1.10.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-render": "craco build --config ./craco-render.config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --write",
    "build:graphapp": "PUBLIC_URL=./ REACT_APP_GRAPHQL_URI=https://graphgist-v3-api.herokuapp.com/graphql npm run build && mv build dist"
  },
  "devDependencies": {
    "@neo4j/code-signer": "^1.1.3",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
